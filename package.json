{
  "name": "anyway-react",
  "version": "1.0.0",
  "description": "Demo & Test Repository for React",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "webpack --env dev --progress --profile --colors",
    "build:prod": "webpack --env prod --progress --profile --colors",
    "fix": "prettier --write 'src/**/*.{ts,tsx}' & tslint --fix 'src/**/*.{ts,tsx}'",
    "start": "node server.js & webpack-dev-server --inline --hot --no-info --env dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yumaeda/anyway-react.git"
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "react"
  ],
  "author": "Yukitaka Maeda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yumaeda/anyway-react/issues"
  },
  "homepage": "https://github.com/yumaeda/anyway-react#readme",
  "dependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-form": "^7.4.2",
    "redux-saga": "^0.16.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.10",
    "@types/react": "^16.7.10",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.10",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux-form": "^7.4.12",
    "css-loader": "^0.28.11",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "json-server": "^0.14.0",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.10.0",
    "prettier": "^1.15.3",
    "redux-devtools-extension": "^2.13.7",
    "sass-loader": "^7.0.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.21.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-webpack-plugin": "^1.3.0",
    "typescript": "^3.2.1",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2"
  }
}
